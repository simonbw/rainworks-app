{
  "expo": {
    "name": "Rainworks",
    "slug": "Rainworks",
    "privacy": "unlisted",
    "description": "View the location of rainworks around the world. https://rain.works/",
    "version": "1.0.12",
    "orientation": "portrait",
    "primaryColor": "#14a9e6",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00000000"
    },
    "assetBundlePatterns": [
      "assets/bundled/*",
      "node_modules/native-base/Fonts/Roboto.ttf",
      "node_modules/native-base/Fonts/Roboto_medium.ttf"
    ],
    "icon": "assets/icon_expo.png",
    "ios": {
      "bundleIdentifier": "com.rainworks.rainworks-ios",
      "buildNumber": "30",
      "icon": "assets/icon_ios.png",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera to upload photos of rainworks.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos to upload photos of rainworks.",
        "NSLocationWhenInUseUsageDescription": "Enabling location services will make it easier to find and submit rainworks near you.",
        "UIUserInterfaceStyle": "light"
      },
      "userInterfaceStyle": "light",
      "splash": {
        "image": "./assets/splash_ios.png",
        "backgroundColor": "#FFFFFF",
        "resizeMode": "contain"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBeju0UvH4Mc44EWb5OGtBOQaEjf7m8GWk"
      }
    },
    "android": {
      "versionCode": 8,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBZ2snsO83-_tYED6ghHrRnBj0zHbsZ7pM"
        }
      },
      "icon": "assets/icon_android.png",
      "package": "works.rain.rainworks",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "splash": {
        "ldpi": "./assets/splash_android.png",
        "mdpi": "./assets/splash_android.png",
        "hdpi": "./assets/splash_android.png",
        "xhdpi": "./assets/splash_android.png",
        "xxhdpi": "./assets/splash_android.png",
        "xxxhdpi": "./assets/splash_android.png",
        "backgroundColor": "#FFFFFF",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ]
  }
}
